{
  "name": "blogtiny",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm dist/*.{js,css}",
    "test": "NODE_PATH=./client mocha --growl --compilers js:babel-core/register,css:test-js/css-null-compiler.js --require ./test-js/test_helper.js $(find client -path '*.spec.js')",
    "dev": "./node_modules/.bin/webpack-dev-server --host 0.0.0.0 --port 8080 --devtool eval --content-base dist/ --progress --colors --hot",
    "build": "NODE_ENV=production NODE_ENV=production ./node_modules/.bin/webpack && npm run zip",
    "server": "source /usr/local/bin/virtualenvwrapper.sh && workon tinyblog && python manage.py runserver -h 0.0.0.0 -p 5000 -r",
    "zip": "zip -r frontend.zip dist"
  },
